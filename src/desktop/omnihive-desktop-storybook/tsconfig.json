{
    "compilerOptions": {
      "allowJs": true,
      "module": "commonjs",
      "skipLibCheck": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "sourceMap": true,
      "baseUrl": ".",
      "outDir": "dist",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "paths": {
        "@withonevision/*": ["../*", "../../server/*", "../../extras/*", "../../other/*"]
      },
      "jsx": "react",
      "target": "ES6"
    }
}