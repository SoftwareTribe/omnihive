{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "module": "ES2020",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "outDir": "dist",
    "paths": {
      "@withonevision/*": ["../*", "../../common/*", "../../extras/*", "../../other/*", "../../server/*"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "ES2020"
  }
}