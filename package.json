{
    "name": "omnihive-workspace",
    "private": true,
    "version": "6.6.6",
    "workspaces": [
        "./common/*",
        "./custom/*",
        "./desktop/*",
        "./extras/*",
        "./other/*",
        "./server/*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/WithOneVisionTechnologies/omnihive.git"
    },
    "scripts": {
        "build:cleanupCoreBuild": "ts-node ./build-cleanupCoreBuild.ts",
        "build:custom": "pnpm node -r ts-node/register ./build-manual.ts -w custom",
        "build:debug": "node --inspect=127.0.0.1:9228 --trace-warnings -r ts-node/register ./build-monorepo.ts -d",
        "build:docs": "cd ./src/other/omnihive-docs && pnpm run build",
        "build:extension": "cd ./src/other/omnihive-vscode-extension && pnpm run build",
        "build:manual": "pnpm node -r ts-node/register ./build-manual.ts",
        "build:monorepo": "pnpm node -r ts-node/register ./build-monorepo.ts",
        "build:release": "standard-version",
        "desktop:debug": "cd ./src/desktop/omnihive-desktop && pnpm run dev",
        "desktop:storybook": "cd ./src/desktop/omnihive-desktop-storybook && pnpm run storybook",
        "docs:debug": "cd ./src/other/omnihive-docs && pnpm start",
        "misc:addDevDeps": "pnpm add @types/chai @types/faker @types/mocha @types/node @typescript-eslint/eslint-plugin @typescript-eslint/parser chai eslint eslint-config-prettier eslint-plugin-prettier faker mocha prettier sinon stylelint stylelint-config-standard typescript -D -E -r",
        "server:debug": "cd ./src/server/omnihive && pnpm run server:debug",
        "taskRunner:debug": "cd ./src/server/omnihive && pnpm run taskRunner:debug",
        "test:debug": "docker-compose -f ./docker-compose.test.yml up -d --force-recreate && nyc mocha --inspect --exit;docker-compose -f ./docker-compose.test.yml down"
    },
    "devDependencies": {
        "@istanbuljs/esm-loader-hook": "0.1.2",
        "@istanbuljs/nyc-config-typescript": "1.0.1",
        "@types/chai": "4.2.22",
        "@types/faker": "5.5.8",
        "@types/fs-extra": "9.0.13",
        "@types/mocha": "9.0.0",
        "@types/node": "16.10.3",
        "@types/rimraf": "3.0.2",
        "@types/tar": "4.0.5",
        "@types/yargs": "17.0.3",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "axios": "0.22.0",
        "chai": "4.3.4",
        "chalk": "4.1.2",
        "cross-env": "7.0.3",
        "enquirer": "2.3.6",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "execa": "5.1.1",
        "faker": "5.5.3",
        "figlet": "1.5.2",
        "fs-extra": "10.0.0",
        "graphql": "15.5.0",
        "listr2": "3.12.2",
        "mocha": "9.1.2",
        "nodemon": "2.0.13",
        "nyc": "15.1.0",
        "prettier": "2.4.1",
        "replace-in-file": "6.2.0",
        "rimraf": "3.0.2",
        "sinon": "11.1.2",
        "source-map-support": "0.5.20",
        "standard-version": "9.3.1",
        "stylelint": "13.13.1",
        "stylelint-config-standard": "22.0.0",
        "tar": "6.1.11",
        "ts-node": "10.2.1",
        "typescript": "4.4.3",
        "yargs": "17.2.1"
    },
    "dependencies": {
        "@12stonechurch/omnihive-worker-analytics": "2.0.76",
        "@12stonechurch/omnihive-worker-auth": "2.0.76",
        "@12stonechurch/omnihive-worker-cloudinary": "2.0.76",
        "@12stonechurch/omnihive-worker-cms": "2.0.76",
        "@12stonechurch/omnihive-worker-communications": "2.0.104",
        "@12stonechurch/omnihive-worker-contacts": "2.0.102",
        "@12stonechurch/omnihive-worker-documents": "2.0.94",
        "@12stonechurch/omnihive-worker-docusign": "2.0.94",
        "@12stonechurch/omnihive-worker-elastic": "2.0.76",
        "@12stonechurch/omnihive-worker-engagements": "2.0.81",
        "@12stonechurch/omnihive-worker-events": "2.0.76",
        "@12stonechurch/omnihive-worker-graphql-builder-v1": "2.0.76",
        "@12stonechurch/omnihive-worker-groups": "2.0.105",
        "@12stonechurch/omnihive-worker-media": "2.0.76",
        "@12stonechurch/omnihive-worker-next": "2.0.76",
        "@12stonechurch/omnihive-worker-old-versions": "2.0.76",
        "@12stonechurch/omnihive-worker-trainings": "2.0.76",
        "@12stonechurch/omnihive-worker-watch": "2.0.76",
        "@12stonechurch/twelvestone-auth-api": "1.10.4",
        "@12stonechurch/twelvestone-common-api": "1.10.4",
        "@12stonechurch/twelvestone-communications-api": "1.10.4",
        "@12stonechurch/twelvestone-contacts-api": "1.10.4",
        "@12stonechurch/twelvestone-engagements-api": "1.10.4",
        "@12stonechurch/twelvestone-events-api": "1.10.4",
        "@12stonechurch/twelvestone-forms-api": "1.10.4",
        "@12stonechurch/twelvestone-groups-api": "1.10.4",
        "@withonevision/omnihive-worker-graphql-builder-v2": "workspace:^6.6.3"
    }
}
